extends layout

block content
  h1= title
  - let image_names = ['star1.jpg','star2.jpg','star3.jpg','star4.jpg','star5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]

      img(src=`images/${iname}` alt='starbucks' style='width:300px; height:300px;')
      |    
  - }